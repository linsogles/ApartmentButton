 * This class is for creating a Tenant object 
 */
package program3;

import java.util.*;
import java.io.*;

public class Tenant 
{
    private int roomNumber;
    private int pinNumber;
    private String tenantName;
            
    public Tenant(int rNum, int pNum, String tenName)
    {
        rNum = 0;
        pNum = 0;
        tenName = "unassigned";
    }
    
    public void setRNum(int rNum)
    {
        rNum = roomNumber;
    }
    
    public void setPNum(int pNum)
    {
        pNum = pinNumber;
    }
    
    public void setTenName(String tenName)
    {
        tenName = tenantName;
    }
    
    public int getRNum()
    {
        return roomNumber;
    }
    
    public int getPNum()
    {
        return pinNumber;
    }
    
    public String getTenName()
    {
        return tenantName;
    }
}
