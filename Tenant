 * This class is for creating a Tenant object 
 */
package program3;

import java.util.*;
import java.io.*;

public class Tenant 
{
    private String roomNumber;
    private String pinNumber;
    private String tenantName;
            
    public Tenant(String rNum, String pNum, String tenName)
    {
        rNum = "unassigned";
        pNum = "unassigned";
        tenName = "unassigned";
    }
    
    public void setRNum(String rNum)
    {
        rNum = roomNumber;
    }
    
    public void setPNum(String pNum)
    {
        pNum = pinNumber;
    }
    
    public void setTenName(String tenName)
    {
        tenName = tenantName;
    }
    
    public String getRNum()
    {
        return roomNumber;
    }
    
    public String getPNum()
    {
        return pinNumber;
    }
    
    public String getTenName()
    {
        return tenantName;
    }
}
