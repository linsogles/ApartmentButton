 */
package program3;

import java.util.*;
import java.io.*;

public class Tenant 
{
    private int roomNumber;
    private int pinNumber;
    private String tenantName;
            
    public Tenant(int rNum, int pNum, String tenName)
    {
        rNum = roomNumber;
        pNum = pinNumber;
        tenName = tenantName;
    }
    
    public String getTenName(int rNum, int pNum)
    {
        return tenantName;
    }
}
